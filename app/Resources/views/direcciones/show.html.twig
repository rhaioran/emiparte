{% extends 'base.html.twig' %}

{% block body %}
    <h1>Direccione</h1>

    <table>
        <tbody>
            <tr>
                <th>Iddireccion</th>
                <td>{{ direccione.iddireccion }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ direccione.direccion }}</td>
            </tr>
            <tr>
                <th>Sigla</th>
                <td>{{ direccione.sigla }}</td>
            </tr>
            <tr>
                <th>Idnivel</th>
                <td>{{ direccione.idnivel }}</td>
            </tr>
            <tr>
                <th>Orden</th>
                <td>{{ direccione.orden }}</td>
            </tr>
            <tr>
                <th>Iduser</th>
                <td>{{ direccione.iduser }}</td>
            </tr>
            <tr>
                <th>Catprog</th>
                <td>{{ direccione.catprog }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('direcciones_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('direcciones_edit', { 'iddireccion': direccione.iddireccion }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
